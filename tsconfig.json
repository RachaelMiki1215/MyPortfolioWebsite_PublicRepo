{
  "compileOnSave": true,
  "compilerOptions": {
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "es5",
    "lib": [
      "ES2015",
      "DOM"
    ],
    "moduleResolution": "Node",
    //"strict": true,
    "module": "ES6",
    "esModuleInterop": true,
    "outDir": "./dist/",
    "noImplicitAny": false,
    "allowJs": true
  }
}
